{
  "name": "boilerplate-api",
  "version": "1.0.0",
  "description": "Simple API Server",
  "main": "src/index.ts or dist/index.js",
  "scripts": {
    "pretty": "prettier --config .prettierrc --write \"{src,test}/**/*.ts\"",
    "lint": "eslint src/**/*.ts && eslint test/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts && eslint test/**/*.ts",
    "test": "jest -i",
    "type-check": "tsc --noEmit",
    "build:js": "babel src --out-dir dist --extensions '.ts'",
    "build": "npm run type-check && npm run build:js",
    "start": "nodemon -e ts -x ts-node -r tsconfig-paths/register src/index.ts",
    "prod": "node dist/."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test}/**/*": [
      "npm run pretty",
      "npm run lint"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@babel/plugin-transform-destructuring": "^7.9.5",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bcrypt-nodejs": "0.0.31",
    "@types/chalk": "^2.2.0",
    "@types/dotenv-safe": "^8.1.0",
    "@types/glob": "^7.1.1",
    "@types/ioredis": "^4.17.2",
    "@types/jest": "^25.2.3",
    "@types/koa": "^2.11.3",
    "@types/koa-jwt": "^3.3.0",
    "@types/koa-morgan": "^1.0.4",
    "@types/koa-router": "^7.4.0",
    "@types/lodash": "^4.14.149",
    "@types/mali": "^0.9.2",
    "@types/node": "^13.11.1",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.0",
    "dotenv-cli": "^3.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.9",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "awilix": "^4.2.5",
    "awilix-express": "^3.0.0",
    "awilix-koa": "^4.0.0",
    "bcrypt": "^5.0.0",
    "bcrypt-nodejs": "0.0.3",
    "chalk": "^4.0.0",
    "class-validator": "^0.12.2",
    "dotenv-safe": "^8.2.0",
    "glob": "^7.1.6",
    "grpc": "^1.24.3",
    "grpc-caller": "^0.13.0",
    "grpc-mock": "^0.7.0",
    "ioredis": "^4.17.3",
    "koa": "^2.11.0",
    "koa-body": "^4.2.0",
    "koa-jsend": "^2.0.0",
    "koa-jwt": "^3.6.0",
    "koa-morgan": "^1.0.1",
    "koa-router": "^8.0.8",
    "koa-simple-healthcheck": "0.0.1",
    "lodash": "^4.17.15",
    "mali": "^0.20.0",
    "mali-logger": "^0.2.2",
    "merge": "^1.2.1",
    "mysql2": "^2.1.0",
    "typeorm": "^0.2.25",
    "typeorm-naming-strategies": "^1.1.0"
  }
}
